package com.imooc;

public class Initail {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Dog dog = new Dog();
		dog.age = 15;
		Dog dog2 = new Dog();
		dog2.age = 15;
		System.out.println(dog);
		if(dog.equals(dog2)){
			System.out.println("两个对象是相同的");
		}else{
			System.out.println("两个对象是不相同的");
		}
	}

}
